{
  "scripts": {
    "test1": "mocha tests/.setup.js tests/**/*-test.js --compilers css:mocha-compiler.js",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "build_client": "rimraf ./dist && cross-env NODE_ENV=production webpack --config webpack.prod.config.js --progress --colors",
    "build_server": "rimraf ./dist_server && cross-env NODE_ENV=production webpack --config webpack.server.config.js --progress --colors",
    "build": "npm run build_client && npm run build_server",
    "dev": "cross-env NODE_ENV=development node devServer",
    "ssr": "cross-env NODE_ENV=production node app"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/__mocks__/**",
      "!**/coverage/**",
      "!**/dist/**",
      "!**/api/**",
      "!**/assets/**",
      "!**/containers/**",
      "!**/middlewares/**",
      "!**/route/**",
      "!**/selectors/**",
      "!**/mapping.js",
      "!**/store/**",
      "!**/utils/**",
      "!**/components/Router/**",
      "!**/reducers/index.js",
      "!**/src/index.jsx",
      "!**/devServer.js",
      "!**/postcss.config.js",
      "!**/webpack.config.js",
      "!**/webpack.dev.config.js",
      "!**/webpack.prod.config.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  },
  "dependencies": {
    "axios": "^0.16.2",
    "axios-mock-adapter": "^1.9.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "npx": "^9.7.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-paginate": "^5.0.0",
    "react-redux": "^5.0.6",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^21.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "copy-webpack-plugin": "^4.2.3",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "enzyme": "^3.1.0",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "ignore-loader": "^0.1.2",
    "jest": "^21.0.1",
    "koa": "^2.3.0",
    "less": "^2.7.1",
    "less-loader": "^4.0.5",
    "postcss-loader": "^2.1.0",
    "postcss-px2rem": "^0.3.0",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.1",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "^0.5.7",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-dev-server": "^2.11.1",
